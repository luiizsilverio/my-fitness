{
  "compilerOptions": {
      "target": "ES2020",
      "outDir": "app/dist/js",
      "noEmitOnError": true,
      "removeComments": true,
      "noImplicitAny": false,
      "strictNullChecks": false,
      "experimentalDecorators": true,
      "sourceMap": false,
      "moduleResolution": "node"
    },
  "include": [
      "app/src/**/*"
  ]
}
